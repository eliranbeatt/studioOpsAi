# Database
DATABASE_URL=postgresql://studioops:studioops123@localhost:5432/studioops

# OpenAI API Key - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Application
APP_ENV=development
APP_DEBUG=true

# Trello Integration - Get from https://trello.com/app-key
TRELLO_API_KEY=your_trello_api_key_here
TRELLO_API_TOKEN=your_trello_api_token_here
TRELLO_DEFAULT_BOARD_ID=your_default_trello_board_id_here

# RAG Configuration
RAG_COLLECTION_NAME=studioops_documents
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Langfuse Observability
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key-here
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key-here
LANGFUSE_HOST=http://localhost:3000

# JWT Authentication
JWT_SECRET_KEY=your-super-secret-jwt-key-here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30