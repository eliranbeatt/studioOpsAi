# StudioOps AI Environment Variables
# Copy this file to .env and update the values

# Database
DATABASE_URL=postgresql://studioops:studioops123@localhost:5432/studioops

# MinIO Object Storage
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=studioops
MINIO_SECRET_KEY=studioops123
MINIO_BUCKET=studioops

# Langfuse Observability
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
LANGFUSE_SECRET_KEY=your-langfuse-secret-key
LANGFUSE_HOST=http://localhost:3002

# OpenAI/LiteLLM
OPENAI_API_KEY=your-openai-api-key
LITELLM_MODEL=gpt-4-turbo-preview

# Trello Integration (for MCP)
TRELLO_API_KEY=your-trello-api-key
TRELLO_TOKEN=your-trello-token

# Application Settings
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=http://localhost:3001

# Development
DEBUG=true
LOG_LEVEL=info